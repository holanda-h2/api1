{
    "version": 2,
    "builds": [{
        "src": "./index.py",
        "use": "@vercel/python"
    }],
    "headers": [{
            "source": "/(.*)",
            "headers": [
                { "key": "Access-Control-Allow-Origin", "value": "*" }
            ]
        },
        {
            "source": "/api/(.*)",
            "headers": [
                { "key": "Access-Control-Allow-Origin", "value": "*" }
            ]
        }
    ]
}